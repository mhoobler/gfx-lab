{
  "name": "wgpu-lab",
  "version": "1.0.0",
  "repository": "git@github.com:mhoobler/wgpu-lab.git",
  "author": "mhoobler <hoobler.michael@gmail.com>",
  "license": "MIT",
  "source": "src/index.html",
  "scripts": {
    "dev": "rm -rf dist .parcel-cache && parcel --no-cache --dist-dir dist src/index.html",
    "build": "rm -rf docs && parcel build src/index.html --dist-dir docs --public-url /wgpu-lab/ && node test-serve.mjs"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@parcel/transformer-less": "2.8.3",
    "@parcel/transformer-typescript-tsc": "^2.8.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "@webgpu/types": "^0.1.32",
    "eslint": "^8.41.0",
    "express": "^4.18.2",
    "jest-environment-jsdom": "^29.5.0",
    "parcel": "^2.8.3",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.4"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testEnvironment": "jsdom",
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    }
  }
}
